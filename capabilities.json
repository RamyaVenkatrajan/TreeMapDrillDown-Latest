    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "dataReductionAlgorithm": {
                        "top": {}
                    },
                    "for": {
                        "in": "category"
                    }
                },
                "values": {
                    "group": {
                        "by": "category",
                        "select": [
                            {
                                "bind": {
                                    "to": "Values1"
                                }
                            },
                            {
                                "bind": {
                                    "to": "Values3"
                                }
                            },
                            {
                                "bind": {
                                    "to": "PercentageValues"
                                }
                            }
                        ]
                    }
                }
            },
            "conditions": [
                {
                    "category":{
                        "max": 1
                    },
                    "Values1": {
                        "max": 5
                    },
                    "Values3": {
                        "max": 5
                    },
                    "PercentageValues": {
                        "max": 1
                    }
                }
            ]
        }
